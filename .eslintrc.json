{
  "extends": ["@rocketseat/eslint-config/react", "next/core-web-vitals"],
  "rules": {
    "no-undef": "off",
    "prettier/prettier": [
      0,
      {
        "printWidth": 95,
        "tabWidth": 2,
        "singleQuote": false,
        "trailingComma": "all",
        "arrowParens": "always",
        "semi": false
      }
    ]
  }
}
